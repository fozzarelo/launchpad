  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && cd build && cp index.html 200.html",
    "build-server": "NODE_ENV=production tsc -p server",
    "start-server": "node build/server/index.js",
    "dev-server": "NODE_ENV=development nodemon --exec \"node -r babel-register -r dotenv/config\" server/index.js",
    "jest": "node scripts/test.js --env=jsdom",
    "jest-once": "node scripts/test.js --env=jsdom --ci --forceExit",
    "flow": "flow",
    "eslint": "eslint ./frontend/ && eslint ./server/",
    "check-prettier": "git ls-files | grep \"frontend\\|server\" | grep \"\\.(css\\|less\\|js\\|json)$\" | grep -v -f .eslintignore | xargs prettier-check --single-quote --trailing-comma all --write --write",
    "prettier": "git ls-files | grep \"frontend\\|server\" | grep \"\\.(css\\|less\\|js\\|json)$\" | grep -v -f .eslintignore | xargs prettier --single-quote --trailing-comma all --write --write",
    "test": "npm run check-prettier && npm run eslint && npm run test-server && npm run test-client",
    "test-server": "jest server",
    "test-client": "jest frontend --env=jsdom",
    "heroku-postbuild": "npm run build-server"
  },
  "jest": {
    "collectCoverageFrom": [
      "frontend/**/*.{js,jsx}",
      "server/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "frontend/services/eject",
      "frontend/services/eslint",
      "server/code.js",
      "types.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/test/setupTests.js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/__tests__/utils/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  }
}
